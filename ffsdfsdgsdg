------------------------------------------------
-- üîò Ch·ª©c nƒÉng ƒê√ìNG/M·ªû menu
------------------------------------------------
local isOpen = true

local function ToggleMenu()
	isOpen = not isOpen
	if isOpen then
		Window:Minimize(false)  -- m·ªü
	else
		Window:Minimize(true)   -- thu g·ªçn
	end
end

-- üì± H·ªó tr·ª£ ƒëi·ªán tho·∫°i (·∫•n v√†o n√∫t)
ImageButton.MouseButton1Click:Connect(ToggleMenu)

-- üíª H·ªó tr·ª£ PC (nh·∫•n ph√≠m End)
game:GetService("UserInputService").InputBegan:Connect(function(input, gp)
	if gp then return end
	if input.KeyCode == Enum.KeyCode.End then
		ToggleMenu()
	end
end)
