-- Trang "Menu"
local Page2 = Instance.new("Frame")
Page2.Parent = Content
Page2.Size = UDim2.new(1, 0, 1, 0)
Page2.BackgroundTransparency = 1
Page2.Visible = false

-- N√∫t trong tab Menu
local function createButton(name, desc, posY, link)
	local Button = Instance.new("TextButton")
	Button.Parent = Page2
	Button.Size = UDim2.new(1, -20, 0, 60)
	Button.Position = UDim2.new(0, 10, 0, posY)
	Button.BackgroundColor3 = Color3.fromRGB(60, 60, 65)
	Button.Text = name .. "\n" .. desc
	Button.TextColor3 = Color3.fromRGB(255, 255, 255)
	Button.Font = Enum.Font.SourceSansBold
	Button.TextSize = 18
	Button.TextYAlignment = Enum.TextYAlignment.Center
	Button.AutoButtonColor = true

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0, 10)
	corner.Parent = Button

	Button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(link))()
	end)
end

createButton("üíé Fram kim c∆∞∆°ng", "auto", 10, "https://raw.githubusercontent.com/caomod2077/Script/refs/heads/main/FNDayFarm.lua")
createButton("üç¨ Candy Farm", "Farm k·∫πo t·ª± ƒë·ªông", 80, "https://raw.githubusercontent.com/MQPS7/99-Night-in-the-Forset/refs/heads/main/CandyFarm")

-- Tab chuy·ªÉn trang
Tab1.MouseButton1Click:Connect(function()
	Page1.Visible = true
	Page2.Visible = false
end)

Tab2.MouseButton1Click:Connect(function()
	Page1.Visible = false
	Page2.Visible = true
end)

-- M·ªü/ƒë√≥ng menu
local open = true
local function toggleMenu()
	open = not open
	if open then
		MainFrame.Visible = true
		TweenService:Create(MainFrame, TweenInfo.new(0.3), {BackgroundTransparency = 0.4}):Play()
	else
		TweenService:Create(MainFrame, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
		task.wait(0.3)
		MainFrame.Visible = false
	end
end

OpenButton.MouseButton1Click:Connect(toggleMenu)
Close.MouseButton1Click:Connect(toggleMenu)
