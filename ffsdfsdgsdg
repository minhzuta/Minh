-- ‚ö° Th√™m ƒëo·∫°n n√†y v√†o CU·ªêI SCRIPT b·∫°n g·ª≠i

-- ü™Ñ N√∫t m·ªü / t·∫Øt menu (mobile & pc)
local Player = game.Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

local ToggleGui = Instance.new("ScreenGui")
ToggleGui.Name = "MenuToggleGui"
ToggleGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ToggleGui.ResetOnSpawn = false
ToggleGui.Parent = PlayerGui

local ToggleButton = Instance.new("ImageButton")
ToggleButton.Parent = ToggleGui
ToggleButton.Size = UDim2.new(0, 60, 0, 60)
ToggleButton.Position = UDim2.new(0.05, 0, 0.25, 0)
ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ToggleButton.Image = "rbxassetid://90677395878536"
ToggleButton.AutoButtonColor = true
ToggleButton.Draggable = true

local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(1, 0)
UICorner.Parent = ToggleButton

-- ‚öôÔ∏è C·∫•u h√¨nh toggle
local menuVisible = true

local function setMenuVisible(state)
	menuVisible = state
	for _, gui in pairs(PlayerGui:GetChildren()) do
		-- ·∫®n ho·∫∑c hi·ªán t·∫•t c·∫£ GUI tr·ª´ n√∫t toggle
		if gui ~= ToggleGui then
			if gui:IsA("ScreenGui") then
				gui.Enabled = menuVisible
			end
		end
	end

	-- Hi·ªáu ·ª©ng ƒë·ªïi m√†u n√∫t khi b·∫≠t/t·∫Øt
	TweenService:Create(ToggleButton, TweenInfo.new(0.25), {
		BackgroundColor3 = menuVisible and Color3.fromRGB(0, 0, 0) or Color3.fromRGB(80, 0, 0)
	}):Play()
end

-- üñ±Ô∏è G√°n s·ª± ki·ªán click (ƒëi·ªán tho·∫°i / PC)
ToggleButton.MouseButton1Click:Connect(function()
	setMenuVisible(not menuVisible)
end)

-- Ban ƒë·∫ßu menu hi·ªÉn th·ªã
setMenuVisible(true)
